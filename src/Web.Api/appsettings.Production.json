{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Core.Api"
    }
  },
  "OAuth": {
    "Google": {
      "ClientId": "__GOOGLE_CLIENT_ID",
      "ClientSecret": "__GOOGLE_CLIENT_SECRET",
      "RedirectUri": "https://api.techinterview.space/api/auth/google/callback"
    },
    "GitHub": {
      "ClientId": "__AUTH_GITHUB_CLIENT_ID",
      "ClientSecret": "__AUTH_GITHUB_CLIENT_SECRET",
      "RedirectUri": "https://api.techinterview.space/api/auth/github/callback"
    },
    "Jwt": {
      "Secret": "__OAUTH_JWT_SECRET"
    }
  },
  "OpenAiApiSecret": "__OPEN_AI_SECRET",
  "SendGridApiKey": "__SENDGRID_API_KEY",
  "ResendComApiKey": "__RESEND_COM_API_KEY",
  "SendGridWebhookSignature": "__SENDGRID_WEBHOOK_SIGNATURE",
  "BaseUrl": "https://techinterview.space",
  "ImageBaseUrl": "https://api.techinterview.space",
  "S3": {
    "BucketName": "__S3_BUCKET_NAME",
    "Region": "",
    "AccessKey": "__S3_ACCESS_KEY",
    "AccessSecret": "__S3_ACCESS_SECRET",
    "S3BaseDomain": "object.pscloud.io",
    "CloudStorageUrl": "https://object.pscloud.io/"
  },
  "Global": {
    "Backend": "https://api.techinterview.space",
    "Frontend": "https://techinterview.space"
  },
  "Frontend": {
    "BaseUrl": "https://techinterview.space",
    "CallbackUrl": "https://techinterview.space/auth-callback"
  },
  "ConnectionStrings": {
    "Database": "__DATABASE_CONNECTION_STRING",
    "Elasticsearch": "http://interviewer-elasticsearch:9200"
  },
  "OpenAI": {
    "ApiKey": "__OPENAI_API_KEY"
  },
  "Claude": {
    "ApiKey": "__CLAUDE_API_KEY"
  }
}
