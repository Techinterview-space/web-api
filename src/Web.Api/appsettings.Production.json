{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Core.Api"
    }
  },
  "IdentityServer": {
    "Audience": "__AUTH0_AUDIENCE",
    "Authority": "https://techinterview-space.eu.auth0.com"
  },
  "Telegram": {
    "BotToken": "__TELEGRAM_BOT_KEY",
    "Enable": true
  },
  "SendGridApiKey": "__SENDGRID_API_KEY",
  "BaseUrl": "https://techinterview.space",
  "ImageBaseUrl": "https://api.techinterview.space",
  "S3": {
    "BucketName": "__S3_BUCKET_NAME",
    "Region": "",
    "AccessKey": "__S3_ACCESS_KEY",
    "AccessSecret": "__S3_ACCESS_SECRET",
    "S3BaseDomain": "object.pscloud.io",
    "CloudStorageUrl": "https://object.pscloud.io/"
  },
  "Global": {
    "Backend": "https://api.techinterview.space",
    "Frontend": "https://techinterview.space"
  },
  "ConnectionStrings": {
    "Database": "__DATABASE_CONNECTION_STRING",
    "Elasticsearch": "http://interviewer-elasticsearch:9200"
  }
}
