{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Core.Api"
    }
  },
  "IdentityServer": {
    "Audience": "{AUTH0_AUDIENCE}",
    "Authority": "https://techinterview-space.eu.auth0.com"
  },
  "SendGridApiKey": "{SENDGRID_API_KEY}",
  "BaseUrl": "https://techinterview.space",
  "ImageBaseUrl": "https://api.techinterview.space",
  "S3": {
    "BucketName": "{S3_BUCKET_NAME}",
    "Region": "",
    "AccessKey": "{S3_ACCESS_KEY}",
    "AccessSecret": "{S3_ACCESS_SECRET}",
    "S3BaseDomain": "object.pscloud.io",
    "CloudStorageUrl": "https://object.pscloud.io/"
  },
  "Global": {
    "Backend": "https://api.techinterview.space",
    "Frontend": "https://techinterview.space"
  },
  "ConnectionStrings": {
    "Database": "{DATABASE_CONNECTION_STRING}",
    "Elasticsearch": "{PROD_ELASTICSEARCH_LINK}"
  }
}
