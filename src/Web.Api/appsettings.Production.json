{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Core.Api"
    },
    "WriteToFileAllowed": true
  },
  "ConnectionStrings": {
    "Database": "Server={DATABASE_HOST};Port={DATABASE_PORT};Database=interviewer_core_prod;User Id={DATABASE_USER};Password={DATABASE_PASS}",
    "Elasticsearch": "http://elastic:{ELASTIC_PASS}@{ELASTIC_HOST}:9200"
  },
  "IdentityServer": {
    "Audience": "core.api",
    "Authority": "https://sso.techinterview.space"
  },
  "BaseUrl": "https://techinterview.space",
  "ImageBaseUrl": "https://api.techinterview.space",
  "MessageBroker": {
    "Host": "kafka.petrelai.xyz:9092",
    "Topics": {
      "UserChange": "user-change-queue-taxer",
      "UserLogin": "user-login-queue-techinterviewer",
      "EmailMessageSend": "email-send-queue-techinterviewer"
    }
  },
  "S3": {
    "BucketName": "tech-interviewer-storage",
    "Region": "",
    "AccessKey": "{S3_ACCESS_KEY}",
    "AccessSecret": "{S3_ACCESS_SECRET}",
    "S3BaseDomain": "object.pscloud.io",
    "CloudStorageUrl": "https://object.pscloud.io/"
  },
  "Global": {
    "Backend": "https://api.techinterview.space",
    "Frontend": "https://techinterview.space"
  }
}
